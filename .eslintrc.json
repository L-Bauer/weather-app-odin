{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "quotes": [
          "error",
          "double"
      ],
      "linebreak-style": ["error", "windows"]
    }
}